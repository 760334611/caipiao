<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/btn_history_time"
    style="@style/base_item_button"
    android:layout_marginBottom="20dp">

    <CheckBox
        android:id="@+id/check_box"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:button="@null"
        android:background="@drawable/check_box_bg"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:visibility="visible"
        />

    <TextView
        android:id="@+id/tv_history_time"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center"
        android:textSize="25dp"
        android:layout_marginHorizontal="3dp"
        android:textColor="@color/text_color0"
        android:maxLines="1"
        android:ellipsize="middle"
        android:text="周五 2023-11-21 18.30.538.30.53"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/check_box"
        app:layout_constraintEnd_toEndOf="parent"
        />
</androidx.constraintlayout.widget.ConstraintLayout>